  </p>
  <p>
    <%= f.label :shift %><br />
    <%= f.datetime_select :shift %>
  </p>
  <p>
    <%= f.label :operator_name %><br />
    <%= f.text_field :operator_name %>
  </p>
  <p>
    <%= f.label :slittingproduction_id %><br />
    <%= f.text_field :slittingproduction_id %>
